Algoritmo Ejercicicio_7
	
	
	//Definir variables
	Definir A,B, resultado Como Entero;
	
	//entrada de datos
	Escribir "definir el valor de A";
	Leer A;
	
	Escribir "definir el valor de B";
	Leer B;
	
	resultado <- (A+B);
	//salida
	Escribir " la suma de A+B es : ", resultado;
	
FinAlgoritmo
